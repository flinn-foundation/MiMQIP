<form action="" class="psychiatricEval">
<h1>Psychiatric Evaluation</h1>
<div class="buttons top">
	<input type="submit" class="orangeBtn" value="Save" />
</div>
<div class="errorMsg">Please enter text in one or more of the fields.</div>
<div class="evalNav">
	<a class="prevEval" href="#"></a>
	<span class="evalDate"></span>
	<a class="nextEval" href="#"></a>
</div>
<ul class="questionList psychiatricEvaluation">
	<li class="fieldWrapper symptom">
		<label for="evaluationsReasons">Reasons for Evaluation</label>
		<textarea id="evaluationsReasons">Enter text...</textarea>
		<span class="inputVal"></span>
	</li>
	<li class="fieldWrapper symptom">
		<label for="presentIllnessHistory">History of Present Illness</label>
		<textarea id="presentIllnessHistory">Enter text...</textarea>
		<span class="inputVal"></span>
	</li>
	<li class="fieldWrapper symptom">
		<label for="pastHistory">Past Psychiatric History</label>
		<textarea id="pastHistory">Enter text...</textarea>
		<span class="inputVal"></span>
	</li>
	<li class="fieldWrapper symptom">
		<label for="substanceAbuseHistory">History of Alcohol and Other Substance Abuse</label>
		<textarea id="substanceAbuseHistory">Enter text...</textarea>
		<span class="inputVal"></span>
	</li>
	<li class="fieldWrapper symptom">
		<label for="generalMedicalHistory">General Medical history</label>
		<textarea id="generalMedicalHistory">Enter text...</textarea>
		<span class="inputVal"></span>
	</li>
	<li class="fieldWrapper symptom">
		<label for="developmentalHistory">Developmental, Psychosocial and Sociocultural History</label>
		<textarea id="developmentalHistory">Enter text...</textarea>
		<span class="inputVal"></span>
	</li>
	<li class="fieldWrapper symptom">
		<label for="occupationalHistory">Occupational and Military History</label>
		<textarea id="occupationalHistory">Enter text...</textarea>
		<span class="inputVal"></span>
	</li>
	<li class="fieldWrapper symptom">
		<label for="legalHistory">Legal History</label>
		<textarea id="legalHistory">Enter text...</textarea>
		<span class="inputVal"></span>
	</li>
	<li class="fieldWrapper symptom">
		<label for="familyHistory">Family History</label>
		<textarea id="familyHistory">Enter text...</textarea>
		<span class="inputVal"></span>
	</li>
	<li class="fieldWrapper symptom">
		<label for="systemsReview">Review of Systems</label>
		<textarea id="systemsReview">Enter text...</textarea>
		<span class="inputVal"></span>
	</li>
	<li class="fieldWrapper symptom">
		<label for="functionalAssessment">Functional Assessment</label>
		<textarea id="functionalAssessment">Enter text...</textarea>
		<span class="inputVal"></span>
	</li>
</ul>
<div class="errorMsg">Please enter text in one or more of the fields.</div>
<div class="evaluatedBy">Evaluated By: <span></span></div>
<div class="buttons bottom">
	<input type="submit" class="orangeBtn" value="Save" />
</div>
</form>